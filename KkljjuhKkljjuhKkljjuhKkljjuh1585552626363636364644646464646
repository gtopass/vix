freedev313.TextColor3 = Color3.fromRGB(255, 255, 255)
freedev313.TextSize = 14

local toggled = false

local function Toggle()
    toggled = not toggled
    if toggled then
        freedev313.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
        freedev313.MouseButton1Click:Connect(function()
            print("ON By freedev ")
        end)
    else
        freedev313.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        freedev313.MouseButton1Click:Disconnect()
    end
end

freedev313.MouseButton1Click:Connect(Toggle)


