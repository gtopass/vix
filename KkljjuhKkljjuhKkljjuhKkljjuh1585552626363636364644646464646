local Players = game:GetService("Players")

local function DisplayPlayerImage(playerId)
    local thumbnailUrl = Players:GetUserThumbnailAsync(playerId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)
    
    local imageLabel = Instance.new("ImageLabel")
    imageLabel.Parent = Tab4Content
    imageLabel.BackgroundTransparency = 1
    imageLabel.Size = UDim2.new(0, 70, 0, 70) 
    imageLabel.Position = UDim2.new(-0.156756753 + 0.04, 0, 0.0882352963 - 0.7, 0) 
    imageLabel.Image = thumbnailUrl 
    
    local imageCorner = Instance.new("UICorner")
    imageCorner.CornerRadius = UDim.new(0, 35) -- ÙÙ Ø¨Ø¶Ø¨Ø· Ø§ÙÙÙÙØ© ÙÙØ§ ÙØªØ­Ø¯ÙØ¯ Ø­Ø¬Ù Ø§ÙØ¯Ø§Ø¦Ø±Ø©
    imageCorner.Name = "ImageCorner"
    imageCorner.Parent = imageLabel
    
    return imageLabel
end

local playerId = 1259903042 
DisplayPlayerImage(playerId)
	
	
